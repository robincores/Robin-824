    "rel8":{"bits":8,"iprel":true,"ipofs":2},
    "rel16":{"bits":16,"endian":"little","iprel":true,"ipofs":3}
  },
  "rules":[
    {"fmt":"add",                "bits":["00010000"]},
    {"fmt":"adjw #~imm8",        "bits":["00000011",0]},
    {"fmt":"aiip ~rel16",        "bits":["11001011",0]},
    {"fmt":"and",                "bits":["00100100"]},
    {"fmt":"b #~imm8",           "bits":["00001010",0]},
    {"fmt":"beq ~rel8",          "bits":["01000010",0]},
    {"fmt":"bge ~rel8",          "bits":["01010110",0]},
    {"fmt":"bgt ~rel8",          "bits":["01010010",0]},
    {"fmt":"ble ~rel8",          "bits":["01001110",0]},
    {"fmt":"bne ~rel8",          "bits":["01000110",0]},
    {"fmt":"dec",                "bits":["01000100"]},
    {"fmt":"div",                "bits":["00011100"]},
    {"fmt":"dup",                "bits":["00001000"]},
    {"fmt":"hlt",                "bits":["11111111"]},
    {"fmt":"i #~imm16",          "bits":["10001011",0]},
    {"fmt":"i0",                 "bits":["10000011"]},
    {"fmt":"i1",                 "bits":["10000111"]},
    {"fmt":"i2b",                "bits":["01001100"]},
    {"fmt":"inc",                "bits":["01000000"]},
    {"fmt":"inv",                "bits":["01011100"]},
    {"fmt":"j ~rel8",            "bits":["01100010",0]},
    {"fmt":"jal ~rel8",          "bits":["01100110",0]},
    {"fmt":"jalr",               "bits":["01101110"]},
    {"fmt":"jr",                 "bits":["01101010"]},
    {"fmt":"lb",                 "bits":["00001001"]},
    {"fmt":"ld",                 "bits":["10001000"]},
    {"fmt":"ldl #~imm8",         "bits":["00001011",0]},
    {"fmt":"lu",                 "bits":["10001001"]},
    {"fmt":"mul",                "bits":["00011000"]},
    {"fmt":"neg",                "bits":["10001001"]},
    {"fmt":"nop",                "bits":["00000000"]},
    {"fmt":"or",                 "bits":["00101000"]},
    {"fmt":"pop1",               "bits":["01111000"]},
    {"fmt":"pop2",               "bits":["01111100"]},
    {"fmt":"rem",                "bits":["00100000"]},
    {"fmt":"sb",                 "bits":["01111001"]},
    {"fmt":"sll",                "bits":["01010000"]},
    {"fmt":"sra",                "bits":["01011000"]},
    {"fmt":"srl",                "bits":["01010100"]},
    {"fmt":"st",                 "bits":["11111000"]},
    {"fmt":"stl #~imm8",         "bits":["01111011",0]},
    {"fmt":"sub",                "bits":["00010100"]},
    {"fmt":"swap",               "bits":["00001100"]},
    {"fmt":"u #~imm8",           "bits":["10001010",0]},
    {"fmt":"xor",                "bits":["00101100"]}
  ]
}
